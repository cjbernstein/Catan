
public class Vertex {
	private int[] resourceTypes;
	private Player owner;
	private boolean	isOccupied; 
	private int 	settlementType; //1= settlement, 2= city
	
	public Vertex(int[] resources){
		resourceTypes = resources;
		isOccupied = false;
	}
	
	public setOwner (Player p){
		owner = p;
		isOccupied = true;
	}
}
